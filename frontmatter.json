{
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "page",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Layout",
          "name": "layout",
          "type": "string",
          "default": "page"
        },
        {
          "title": "Permalink",
          "name": "permalink",
          "type": "string"
        }
      ]
    },
    {
      "name": "post",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Layout",
          "name": "layout",
          "type": "string",
          "default": "post"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "All Pages",
      "path": "[[workspace]]"
    }
  ],
  "frontMatter.content.publicFolder": "assets/images",
  "frontMatter.preview.host": "http://localhost:4000"
}